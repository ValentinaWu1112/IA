(3,4) -> (6,-1)(4,1)
(7,2) -> (2,-1)(4,3)
(2,-1) -> (7,2)(-3,-1)
(-1,-2) -> (4,-2)(-3,-1)
(4,3) -> (1,3)(7,2)
(-3,-1) -> (2,-1)(-1,-2)
(4,1) -> (-1,1)(3,4)

